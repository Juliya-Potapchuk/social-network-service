{
  "env": {
    "browser": true
  },
    "extends": [
      "airbnb-base",
      "react-app",
      "airbnb",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/prop-types": 0,
      "no-shadow": "off",
     "no-unused-expressions": "off",
     "import/prefer-default-export": "off"
    }
  }